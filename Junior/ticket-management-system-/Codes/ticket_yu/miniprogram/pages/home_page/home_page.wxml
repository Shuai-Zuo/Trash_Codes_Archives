
  
<view>
  <SearchInput></SearchInput>
  <view class="index_swiper">
    <swiper autoplay indicator-dots circular>
      <swiper-item
      wx:for="{{swiperList}}"
      wx:key="id"

      >
        <view>
          <image src="{{item.image_src}}" mode="widthFix"/>
            
        </view>  
      </swiper-item>
    </swiper>
  </view>
  <Groups groups="{{groups}}">  </Groups>
  <showInfo showInfo="{{performList}}"></showInfo>

</view>

  
  
